{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*":["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@routers/*": ["./src/routers/*"],
      "@screens/*": ["./src/screens/*"],
      "@storage/*":["./src/storage/*"],
      "@theme/*": ["./src/theme/*"],
      "@utils/*": ["./src/utils/*"]
    }
  }
}
